{
  "name": "olachat-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node devServer.js",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "build:demo": "NODE_ENV=production webpack --config webpack.config.demo.js --progress",
    "build:search": "NODE_ENV=production webpack --config webpack.config.search.js --progress",
    "build:staging": "OLA_ENV=staging NODE_ENV=production webpack --config webpack.config.demo.js --progress",
    "main": "NODE_ENV=production webpack --config webpack.config.main.js",
    "iframe": "NODE_ENV=production webpack --config webpack.config.iframe.js",
    "demo": "npm run main && npm run build:demo && cat ./../polyfill.min.js ./../demo/olasearch.core.min.js ./../demo/olasearch.config.min.js ./../demo/olachat.min.js > ./../demo/olachat.all.min.js",
    "search": "npm run main && npm run build:search && cat ./../polyfill.min.js ./../demo/olasearch.core.min.js ./../demo/olasearch.config.min.js ./../demo/olachat.search.min.js > ./../demo/olachat.search.all.min.js",
    "staging": "npm run main && npm run build:staging && cat ./../polyfill.min.js ./../demo/olasearch.core.min.js > ./../demo/olachat.all.staging.min.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "expose-loader": "^0.7.3",
    "express": "^4.15.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "prop-types": "^15.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "dependencies": {
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  }
}
